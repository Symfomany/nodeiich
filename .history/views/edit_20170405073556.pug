extends layout.pug

block content
    h1 Editer la page <strong>#{page.clef}</strong>
    form
        .form-group
            label(for='content') Contenu
            |  
            input#content.form-control(type='text', value="#{page.valeur}" placeholder='Email')
        |  
        button.btn.btn-default(type='submit') Submit
